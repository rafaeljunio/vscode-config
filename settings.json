{
  "cSpell.enableFiletypes": [
    "!asciidoc",
    "!c",
    "!cpp",
    "!csharp",
    "!go",
    "!handlebars",
    "!haskell",
    "!jade",
    "!java",
    "!latex",
    "!php",
    "!pug",
    "!python",
    "!restructuredtext",
    "!rust",
    "!scala",
    "!scss"
  ],
  "cSpell.userWords": [
    "bootcamp",
    "chakra",
    "checkin",
    "checkins",
    "clsx",
    "Codegen",
    "datadog",
    "Datetime",
    "dayjs",
    "Dotenv",
    "Elysia",
    "esbuild",
    "fastify",
    "Fastify",
    "feedbackwidget",
    "ffprobe",
    "Hasher",
    "Hono",
    "ilike",
    "IUGU",
    "jamjuree",
    "jupiter",
    "kinde",
    "liveblocks",
    "LIVEBLOCKS",
    "Marguerita",
    "middlewares",
    "mixpanel",
    "monaco",
    "nestjs",
    "nextjs",
    "novu",
    "omni",
    "Omni",
    "Onboarded",
    "pallas",
    "postgres",
    "postgresql",
    "prefetch",
    "reactflow",
    "roboto",
    "rocketseat",
    "rotion",
    "rsxp",
    "Sandpack",
    "shiki",
    "skylab",
    "sqlite",
    "supergraph",
    "svgr",
    "sympla",
    "tailwindcss",
    "textblock",
    "tiptap",
    "trpc",
    "TRPC",
    "tsup",
    "unfollow",
    "Unfollow",
    "unform",
    "Unform",
    "unmark",
    "upsert",
    "Usuario",
    "WEBPUSH"
  ],
  "cSpell.language": "en,pt",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.addMissingImports": "explicit"
  },
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorStyle": "block",
  "workbench.colorCustomizations": {
    "editorCursor.foreground": "#F4D93A"
  },
  // "editor.fontFamily": "Monaco",
  // "editor.fontFamily": "Cascadia Code, Consolas",
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontLigatures": "'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'zero', 'onum'",
  // "editor.fontLigatures": true,
  "editor.fontSize": 15,
  // "editor.hideCursorInOverviewRuler": true,
  "editor.lineHeight": 2.0,
  // "editor.letterSpacing": 0.4,
  // "editor.fontWeight": "200",
  "editor.fontWeight": "100",
  "editor.minimap.enabled": true,
  "editor.parameterHints.enabled": true,
  "editor.renderLineHighlight": "gutter",
  // "editor.renderWhitespace": "boundary",
  // "editor.rulers": [
  //   80,
  //   120
  // ],
  "editor.suggestSelection": "first",
  "editor.semanticHighlighting.enabled": false,
  "editor.tabSize": 2,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "emmet.syntaxProfiles": {
    "javascript": "jsx"
  },
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.sortOrder": "foldersNestsFiles",
  "explorer.confirmDragAndDrop": false,
  "explorer.fileNesting.enabled": false,
  "explorer.fileNesting.patterns": {
    "package.json": ".eslint*, prettier*, tsconfig*, vite*, pnpm-lock*, bun.lockb, nest*",
    "tailwind.config.js": "tailwind.config*, postcss.config*",
    ".env.local": ".env*",
    ".env": ".env*"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "graphql"
  ],
  "extensions.ignoreRecommendations": true,
  "files.exclude": {
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/.git": true,
    "**/.trunk/*actions/": true,
    "**/.trunk/*logs/": true,
    "**/.trunk/*notifications/": true,
    "**/.trunk/*out/": true,
    "**/.trunk/*plugins/": true
  },
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "always",
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.codeLens.authors.enabled": false,
  "javascript.suggest.autoImports": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "files.associations": {
    ".env.*": "dotenv",
    ".prettierrc": "json",
    "*.css": "css",
    "*.ts": "typescript"
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "redhat.telemetry.enabled": true,
  "RainbowBrackets.depreciation-notice": false,
  "security.promptForLocalFileProtocolHandling": false,
  "security.workspace.trust.untrustedFiles": "newWindow",
  "tabnine.experimentalAutoImports": true,
  "tailwindCSS.experimental.classRegex": [
    [
      "tv\\(([^)]*)\\)",
      "[\"'`]([^\"'`]*).*?[\"'`]"
    ]
  ],
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "terminal.integrated.fontSize": 13,
  // "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font",
  "terminal.integrated.fontFamily": "Fira Code Nerd Font Mono",
  "terminal.integrated.gpuAcceleration": "off",
  "terminal.integrated.showExitAlert": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.suggest.autoImports": true,
  "typescript.tsserver.log": "off",
  "typescript.preferences.includePackageJsonAutoImports": "on",
  "typescript.preferences.preferTypeOnlyAutoImports": true,
  "update.mode": "start",
  "update.showReleaseNotes": false,
  "window.commandCenter": true,
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.labelFormat": "short",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.layoutControl.enabled": false,
  "workbench.tree.enableStickyScroll": false,
  "editor.stickyScroll.enabled": false,
  "files.watcherExclude": {
    "**/.trunk/*actions/": true,
    "**/.trunk/*logs/": true,
    "**/.trunk/*notifications/": true,
    "**/.trunk/*out/": true,
    "**/.trunk/*plugins/": true
  },
  "console-ninja.featureSet": "Community",
  "workbench.iconTheme": "vscode-great-icons",
  "workbench.colorTheme": "Houston",
  "indenticator.width": 2,
  // "editor.defaultFormatter": "biome",
  "[javascript]": {
    "editor.defaultFormatter": "biomejs.biome"
    // "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "trunk.showPreexistingIssues": true,
  "GitLive.Issue tracker integration": "Disabled",
  "git.autofetch": true,
  "GitLive.Special branches": "main|master|trunk|dev|develop|qa|test|release[-/].*",
  "workbench.editor.customLabels.patterns": {
    "**/{layout,page}.tsx": "${dirname} ${filename}"
  },
  "window.confirmSaveUntitledWorkspace": false,
  "workbench.sideBar.location": "left",
  "importSorter.generalConfiguration.sortOnBeforeSave": true,
}
